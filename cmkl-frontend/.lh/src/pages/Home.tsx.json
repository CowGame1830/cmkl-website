            "content": "import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { useTheme } from '../context/ThemeContext';\r\nimport { products, blogPosts } from '../data/mockData';\r\nimport ProductCard from '../components/ProductCard';\r\nimport BlogCard from '../components/BlogCard';\r\nimport './Home.css';\r\n\r\nconst Home: React.FC = () => {\r\n  const { theme } = useTheme();\r\n  \r\n  const featuredProducts = products.slice(0, 4);\r\n  const latestPosts = blogPosts.slice(0, 3);\r\n\r\n  return (\r\n    <div className={`home-page ${theme}`}>\r\n      {/* Hero Section */}\r\n      <section className=\"hero\">\r\n        <div className=\"hero-content\">\r\n          <h1 className=\"hero-title\">Welcome to SampleSite</h1>\r\n          <p className=\"hero-subtitle\">\r\n            Discover amazing products and insightful articles in our modern web experience\r\n          </p>\r\n          <div className=\"hero-actions\">\r\n            <Link to=\"/products\" className=\"btn btn-primary\">Shop Now</Link>\r\n            <Link to=\"/blog\" className=\"btn btn-secondary\">Read Blog</Link>\r\n          </div>\r\n        </div>\r\n        <div className=\"hero-image\">\r\n          <img \r\n            src=\"https://images.unsplash.com/photo-1441986300917-64674bd600d8?w=600&h=400&fit=crop\" \r\n            alt=\"Modern workspace\"\r\n          />\r\n        </div>\r\n      </section>\r\n\r\n      {/* Featured Products */}\r\n      <section className=\"featured-products\">\r\n        <div className=\"container\">\r\n          <div className=\"section-header\">\r\n            <h2>Featured Products</h2>\r\n            <Link to=\"/products\" className=\"view-all-link\">View All Products →</Link>\r\n          </div>\r\n          \r\n          <div className=\"products-grid\">\r\n            {featuredProducts.map(product => (\r\n              <ProductCard key={product.id} product={product} />\r\n            ))}\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      {/* Latest Blog Posts */}\r\n      <section className=\"latest-posts\">\r\n        <div className=\"container\">\r\n          <div className=\"section-header\">\r\n            <h2>Latest Articles</h2>\r\n            <Link to=\"/blog\" className=\"view-all-link\">View All Posts →</Link>\r\n          </div>\r\n          \r\n          <div className=\"blog-grid\">\r\n            {latestPosts.map(post => (\r\n              <BlogCard key={post.id} post={post} />\r\n            ))}\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      {/* CTA Section */}\r\n      <section className=\"cta-section\">\r\n        <div className=\"container\">\r\n          <div className=\"cta-content\">\r\n            <h2>Ready to Get Started?</h2>\r\n            <p>Join thousands of satisfied customers and discover what makes us special.</p>\r\n            <Link to=\"/about\" className=\"btn btn-primary\">Learn More</Link>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Home;"
